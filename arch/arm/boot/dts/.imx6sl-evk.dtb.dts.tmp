    0x180 0x488 0x000 0x5 0x0 0x80000000
    0x078 0x368 0x000 0x5 0x0 0x17000
    0x07c 0x36c 0x000 0x5 0x0 0x17000
    0x084 0x374 0x000 0x5 0x0 0x17000
    0x138 0x428 0x000 0x5 0x0 0x1b0b0
    0x0f0 0x3e0 0x000 0x5 0x0 0x17000
    0x0ec 0x3dc 0x000 0x5 0x0 0x80000000
    0x184 0x48c 0x000 0x5 0x0 0x110b0
    0x218 0x520 0x000 0x5 0x0 0x1b0b0
   >;
  };

                pinctrl_audmux: audmuxgrp {
                        fsl,pins = <
                                0x054 0x2ac 0x000 0x0 0x0 0x4130b0
                                0x05c 0x2b4 0x000 0x0 0x0 0x4130b0
