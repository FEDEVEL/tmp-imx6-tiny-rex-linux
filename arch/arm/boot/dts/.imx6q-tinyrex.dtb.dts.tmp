  cpu@1 {
   compatible = "arm,cortex-a9";
   device_type = "cpu";
   reg = <1>;
   next-level-cache = <&L2>;
  };

  cpu@2 {
   compatible = "arm,cortex-a9";
   device_type = "cpu";
