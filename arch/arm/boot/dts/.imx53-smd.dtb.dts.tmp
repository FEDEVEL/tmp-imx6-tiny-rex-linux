   compatible = "fsl,imx53-ipu";
   reg = <0x18000000 0x08000000>;
   interrupts = <11 10>;
   clocks = <&clks 59>, <&clks 110>, <&clks 61>;
   clock-names = "bus", "di0", "di1";
   resets = <&src 2>;
  };

